<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <VersionPrefix>6.5.0</VersionPrefix>
    <Description>JSON for F#</Description>
    <Authors>Andrew Cherry;Michael Newton;Henrik Feldt;Marcus Griep;Tony Williams</Authors>
    <Description>Monadic JSON serialization/deserialization library for F#</Description>
    <Owners>Andrew Cherry</Owners>
    <Copyright>Copyright Â© Xyncro Ltd 2018</Copyright>
    <Authors>Andrew Cherry, Michael Newton, Henrik Feldt, Marcus Griep, Tony Williams</Authors>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageTags>functional, json, f#, optics</PackageTags>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>  

    <!-- SourceLink related properties https://github.com/dotnet/SourceLink#using-sourcelink -->
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-*" PrivateAssets="All" />
  </ItemGroup>
</Project>
